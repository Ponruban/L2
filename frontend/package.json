    "@mui/material": "^5.15.0",
    "@mui/x-date-pickers": "^8.9.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@tanstack/react-query": "^5.84.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "postcss": "^8.5.6",
